{
	"name": "sunfoxcz/price-calculator",
	"type": "library",
	"description": "Library for easier price calculation using VAT and discount.",
	"keywords": ["price", "calculator", "calculation", "VAT", "DPH", "TAX", "discount", "reduction"],
	"license": "MIT",
	"authors": [
		{
			"name": "Tomáš Jacík",
			"email": "tomas.jacik@sunfox.cz",
			"homepage": "http://sunfox.cz"
		}
	],
	"require": {
		"php": ">=8.1",
		"nette/utils": "^4.0"
	},
	"suggest": {
		"nette/bootstrap": "^3.2"
	},
	"require-dev": {
		"nette/tester": "^2.5",
		"nette/bootstrap": "^3.2",
		"phpstan/phpstan": "^1.11"
	},
	"autoload": {
		"psr-4": {
			"Sunfox\\PriceCalculator\\": "src/"
		}
	},
	"autoload-dev": {
